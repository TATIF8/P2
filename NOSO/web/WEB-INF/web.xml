<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>Logserv</servlet-name>
        <servlet-class>Servs.SERV</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Signin</servlet-name>
        <servlet-class>Servs.Signin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Signserv</servlet-name>
        <servlet-class>Servs.Signserv</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlog</servlet-name>
        <servlet-class>Servs.Servlog</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Logserv</servlet-name>
        <url-pattern>/Logserv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Signin</servlet-name>
        <url-pattern>/Signin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Signserv</servlet-name>
        <url-pattern>/Signserv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlog</servlet-name>
        <url-pattern>/Servlog</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
